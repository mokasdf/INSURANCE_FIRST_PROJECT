    @* for upload images *@
    <script src="~/DashbordAssets/js/file-upload.js"></script>

    <script>
        document.getElementById("customFileInput").addEventListener("change", function () {
            const input = this;
            const label = document.querySelector(".custom-label");
            const fileNameDisplay = document.getElementById("chosenFileName");

            if (input.files.length > 0) {
                fileNameDisplay.value = input.files[0].name;
            } else {
                fileNameDisplay.value = "";
            }
        });
    </script>

